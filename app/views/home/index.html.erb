<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>


<div class="rakuten-search-form my-3">
  <%= form_with url: rakuten_searches_path, method: :get, local: true, class: "form-inline" do |f| %>
    <%= f.text_field :keyword, value: params[:keyword], placeholder: "상품 검색", class: "form-control mr-2" %>
    <%= f.submit "검색", class: "btn btn-outline-success" %>
  <% end %>
</div>